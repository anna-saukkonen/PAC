profiles {
    docker {
        process.container = "annasaukkonen/pac_pipeline:v1"
        docker.enabled = true
    }
    singularity {
        process.container = "annasaukkonen/pac_pipeline:v1"
        singularity.autoMounts = true
        singularity.enabled = true
    }
}

params {
    genome_version      = ''
    id                  = ''
    variants            = ''
    reads               = '' 
    igenomes_path       = 's3://ngi-igenomes/igenomes/'
    awsregion           = 'eu-west-1'
    cpus                = 10
    outdir              = './pac_results'
    

}

includeConfig 'conf/genome_version.config'
